DROP TABLE MMH_MEMBER;
DROP SEQUENCE M_NOSEQ;
DROP SEQUENCE MMH_MEMBER_SEQ;

CREATE SEQUENCE MMH_MEMBER_SEQ;

ALTER TABLE MMH_MEMBER
ADD M_ADDRESS2 VARCHAR2(200);

CREATE TABLE MMH_MEMBER(
M_NO NUMBER PRIMARY KEY,
M_ID VARCHAR2(20) NOT NULL,
M_PW VARCHAR2(20) NOT NULL,
M_NAME VARCHAR2(20),
M_EMAIL VARCHAR2(40) NOT NULL,
M_ADDRESS VARCHAR2(200),
M_ADDRESS2 VARCHAR2(200),
M_JOIN CHAR(1) CHECK (M_JOIN IN('Y','N')),
M_GRADE CHAR(2) CHECK(M_GRADE IN ('U','A'))
);

SELECT * FROM MMH_MEMBER
order by m_no;


SELECT M_ID FROM MMH_MEMBER
			WHERE M_ID = 'ADMIN2';

SELECT M_ID FROM MMH_MEMBER
			WHERE M_ID = admin2;

INSERT INTO MMH_MEMBER
VALUES (MMH_MEMBER_SEQ.NEXTVAL, 'ADMIN2', '1234', '관리자2', 'ADMIN.COM', '도로명', '상세주소','Y', 'A');

		