<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="sample">

	<resultMap type="memberDto" id="memberMap">
		<result property="m_no" column="M_NO" />
		<result property="m_id" column="M_ID" />
		<result property="m_pw" column="M_PW" />
		<result property="m_name" column="M_NAME" />
		<result property="m_email" column="M_EMAIL" />
		<result property="m_address" column="M_ADDRESS" />
		<result property="m_join" column="M_JOIN" />
		<result property="m_grade" column="M_GRADE" />
	</resultMap>
	<select id="selectList" resultMap="boardMap">
		SELECT MYNO, MYNAME, MYTITLE,MYCONTENT, MYDATE
		FROM MYBOARD
		ORDER BY MYNO DESC
	</select>

	<select id="selectOne" resultMap="boardMap" parameterType="int">
		SELECT MYNO, MYNAME, MYTITLE, MYCONTENT, MYDATE
		FROM MYBOARD
		WHERE MYNO=#{myno}
	</select>

	<insert id="insert" parameterType="boardDto">
		INSERT INTO MYBOARD
		VALUES(MYNOSEQ.NEXTVAL, #{myname}, #{mytitle}, #{mycontent},SYSDATE)
	</insert>

	<update id="update" parameterType="boardDto">
		UPDATE MYBOARD
		SET MYNAME =
		#{myname}, MYTITLE= #{mytitle}, MYCONTENT= #{mycontent}, MYDATE =
		SYSDATE
		WHERE MYNO = #{myno}
	</update>

	<delete id="delete" parameterType="int">
		DELETE MYBOARD
		WHERE MYNO
		=#{myno}
	</delete>

</mapper>
